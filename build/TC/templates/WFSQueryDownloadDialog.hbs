<div class="tc-ctl-wfsquery-dialog tc-ctl-wfsquery-dl-dialog tc-modal">
    <div class="tc-modal-background tc-modal-close"></div>
    <div class="tc-modal-window">
        <div class="tc-modal-header">
            <h3>{{i18n "feature"}} - {{i18n "download"}}</h3>
            <div class="tc-modal-close"></div>
        </div>
        <div class="tc-modal-body">
            {{#each elevation}}
            <div class="tc-ctl-wfsquery-dialog-elev">
                <input id="{{checkboxId}}" type="checkbox">
                <label for="{{checkboxId}}" class="tc-ctl-wfsquery-dialog-elev-label">{{i18n "includeElevations"}}</label>
            </div>
                {{#if resolution}}
            <div class="tc-ctl-wfsquery-dialog-ip tc-hidden">
                <h4>{{i18n "interpolateCoordsFromElevProfile"}}</h4>
                <label>
                    <input type="radio" name="finfo-ip-coords" value="0" checked />
                    <span>{{i18n "no"}}</span>
                </label>
                <label>
                    <input type="radio" name="finfo-ip-coords" value="1"/>
                    <span>{{i18n "yes"}}</span>
                </label>
                <div class="tc-ctl-wfsquery-dialog-ip-m tc-hidden">{{i18n "interpolateEveryXMeters.1"}}<input type="number" min="1" step="1" class="tc-textbox" value="{{resolution}}" />{{i18n "interpolateEveryXMeters.2"}}</div>
            </div>
                {{/if}}
            {{/each}}
            <div class="tc-ctl-wfsquery-dialog-dl">
                <div>
                    <button class="tc-button tc-btn-dl tc-ctl-wfsquery-dl-btn-kml" data-format="KML" title="KML">KML</button>
                    <button class="tc-button tc-btn-dl tc-ctl-wfsquery-dl-btn-gml" data-format="GML" title="GML">GML</button>
                    <button class="tc-button tc-btn-dl tc-ctl-wfsquery-dl-btn-geojson" data-format="GeoJSON" title="GeoJSON">GeoJSON</button>
                    <button class="tc-button tc-btn-dl tc-ctl-wfsquery-dl-btn-wkt" data-format="WKT" title="WKT">WKT</button>
                    <button class="tc-button tc-btn-dl tc-ctl-wfsquery-dl-btn-gpx" data-format="GPX" title="GPX">GPX</button>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="tc-ctl-wfsquery-dialog tc-ctl-wfsquery-share-dialog tc-modal">
    <div class="tc-modal-background tc-modal-close"></div>
    <div class="tc-modal-window">
        <div class="tc-modal-header">
            <h3>{{i18n "feature"}} - {{i18n "share"}}</h3>
            <div class="tc-modal-close"></div>
        </div>
        <div class="tc-modal-body">
            <div class="tc-ctl-wfsquery-share-dialog-ctl"></div>
        </div>
    </div>
</div>


