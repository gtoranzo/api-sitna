<h2>{{i18n "backgroundMaps"}}</h2>
<div class="tc-ctl-bms-tree">
    <form>
        <ul class="tc-ctl-bms-branch">
            {{#each baseLayers}}
            {{> tc-ctl-bms-node }}
            {{/each}}
            {{#if dialogMore}}
            <li class="tc-ctl-bms-node"><label class="tc-ctl-bms-more-node" title="{{i18n "moreBackgroundMaps"}}"><input type="radio" name="bms" value="moreLayers"><span></span></label></li>
            {{/if}}
        </ul>
    </form>
</div>